{
    "name": "modus/laravel-microsoft-sso",
    "description": "A easy way to integrate logging in with microsoft",
    "keywords": ["microsoft", "sso", "laravel"],
    "version": "0.1.2",
    "type": "library",
    "require": {
        "php": ">=8.0",
        "onelogin/php-saml": "4.1.0",
        "laravel/prompts": "^0.1.23"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Modus\\LaravelMicrosoftSso\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Modus\\LaravelMicrosoftSso\\SsoServiceProvider"
            ]
        }
    },
    "authors": [
        {
            "name": "Alex van Steenhoven",
            "email": "alex@modus-digital.com",
            "homepage": "https://modus-digital.com",
            "role": "Developer"
        }
    ],
    "minimum-stability": "stable"
}
